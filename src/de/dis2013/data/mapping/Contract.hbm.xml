<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="de.dis2013.data.Contract" table="Contract">
        <id name="contractid" column="id">
            <generator class="increment" />
        </id>
        <property name="settelmentplace" type="string" column="settlemtnplace" />
        <property name="contractdate" type="string" column="contractdate" />
        <one-to-one name="purchaseContract" class="de.dis2013.data.PurchaseContract" cascade="save-update"></one-to-one>
        <one-to-one name="tenancyContract" class="de.dis2013.data.TenancyContract" cascade="save-update"></one-to-one>
    </class>
</hibernate-mapping>