<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="de.dis2013.data.House" table="house">
        <id name="estateid" column="id">
            <generator class="foreign">
                <param name="property">estate</param>
            </generator>
        </id>
        <one-to-one name="estate" class="de.dis2013.data.Estate" constrained="true"></one-to-one>
        <property name="floors" type="int" column="floors" />
        <property name="price" type="int" column="price" />
        <property name="garden" type="int" column="garden" />
    </class>
</hibernate-mapping>