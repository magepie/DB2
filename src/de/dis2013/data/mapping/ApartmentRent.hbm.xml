<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="de.dis2013.data.ApartmentRent" table="appartmentrent">
        <id name="contractid" type="java.lang.Integer">
            <column name="contractid" />
            <generator class="foreign">
                <param name="property">contract</param>
            </generator>
        </id>
        <one-to-one name="contract" foreign-key="contractid" class="de.dis2013.data.Contract" constrained="true"></one-to-one>
        <one-to-one name="owner" foreign-key="ownerid" class="de.dis2013.data.Owner" constrained="true"></one-to-one>
        <one-to-one name="estate" foreign-key="estateid" class="de.dis2013.data.Estate" constrained="true"></one-to-one>
    </class>
</hibernate-mapping>



